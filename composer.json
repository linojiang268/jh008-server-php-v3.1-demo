{
  "name": "zero2all/jihe-server",
  "description": "Brand new version of Jihe server",
  "minimum-stability": "stable",
  "license": "proprietary",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.1.4",
    "brouwers/laravel-doctrine": "1.0.4",
    "gedmo/doctrine-extensions": "2.4.5",
    "zero2all/alioss": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.6",
    "phpunit/dbunit": "1.4.1",
    "ramsey/uuid": "2.8.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Jihe\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Jihe\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
